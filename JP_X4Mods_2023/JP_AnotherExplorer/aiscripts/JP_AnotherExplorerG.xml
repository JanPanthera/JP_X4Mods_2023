<?xml version="1.0" encoding="utf-8"?>

<aiscript name="JP_AnotherExplorerG" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="aiscripts.xsd" version="1">

   <order id="JP_AnotherExplorerG" name="_{8888888, 97}AnotherExplorer - {8888888, 99}{8888888, 98}{20001, 901}{8888888, 99}" description="{8888888, 471}" category="navigation" infinite="true" allowinloop="false">
      <params>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_000" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="JUMP_GATES"              text="{8888888, 1}"   comment="Jump Gates?"             type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{1} else true"/>
         <param name="ACTIVE_JUMP_GATES"       text="- {8888888, 2}" comment="Active Jump Gates?"      type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{2} else true"/>
         <param name="INACTIVE_JUMP_GATES"     text="- {8888888, 3}" comment="Inactive Jump Gates?"    type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{3} else true"/>
         <param name="ACCELERATORS"            text="{8888888, 4}"   comment="Accelerators?"           type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{4} else true"/>
         <param name="ACTIVE_ACCELERATORS"     text="- {8888888, 5}" comment="Active Accelerators?"    type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{5} else true"/>
         <param name="INACTIVE_ACCELERATORS"   text="- {8888888, 6}" comment="Inactive Accelerators?"  type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{6} else true"/>
         <param name="SUPER_HIGHWAYS"          text="{8888888, 7}"   comment="Superhighways?"          type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{7} else true"/>
         <param name="ACTIVE_SUPER_HIGHWAYS"   text="- {8888888, 8}" comment="Active Superhighways?"   type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{8} else true"/>
         <param name="INACTIVE_SUPER_HIGHWAYS" text="- {8888888, 9}" comment="Inactive Superhighways?" type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{9} else true"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_002" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="STATIONS"             text="{8888888, 10}"   comment="Stations?"                    type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{10} else true"/>
         <param name="WHARFS"               text="- {8888888, 11}" comment="Wharfs?"                      type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{11} else true"/>
         <param name="SHIPYARDS"            text="- {8888888, 12}" comment="Shipyards?"                   type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{12} else true"/>
         <param name="EQUIPMENTDOCKS"       text="- {8888888, 13}" comment="Equipment docks?"             type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{13} else true"/>
         <param name="TRADING_STATIONS"     text="- {8888888, 14}" comment="Trading stations?"            type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{14} else true"/>
         <param name="PIRATE_BASES"         text="- {8888888, 15}" comment="Pirate bases?"                type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{15} else true"/>
         <param name="RECYCLING_STATIONS"   text="- {8888888, 16}" comment="Scrap processing facilities?" type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{16} else true"/>
         <param name="DEFENCE_STATIONS"     text="- {8888888, 17}" comment="Defense stations?"            type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{17} else true"/>
         <param name="FACTION_HEADQUARTERS" text="- {8888888, 18}" comment="Faction Headquarters?"        type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{18} else true"/>
         <param name="NON_SPECIAL_STATIONS" text="- {8888888, 19}" comment="Factories?"                   type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{19} else true"/>
         <param name="CONSTRUCTION_SITES"   text="- {8888888, 20}" comment="Construction Sites?"          type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{20} else true"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_995" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="OWNERLESS_SECTORS" text="{8888888, 30}" comment="Ownerless Sectors?" type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{21} else true"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_996" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="IDLE_TIME" text="{8888888, 80}" comment="Idle Time" type="time" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{22} else 600s">
            <input_param name="min" value="60s"/>
            <input_param name="max" value="3600s"/>
            <input_param name="step" value="10s"/>
         </param>
         <param name="IDLE_MOVE_TO" text="{8888888, 81}" comment="Idle: Move to?" type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{23} else true"/>
         <param name="WHERE_TO_MOVE" text="- {8888888, 82}" comment="Where to move?" type="position" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{24} else [this.ship.sector, this.ship.sector.coreposition]">
            <input_param name="class" value="class.sector"/>
         </param>
         <param name="START_POSITION" type="internal" default="[this.ship.sector, this.ship.position]"/>
         <param name="IDLE_FOLLOW" text="{8888888, 83}" comment="Idle: Follow?" type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{25} else false"/>
         <param name="WHO_TO_FOLLOW" text="- {8888888, 84}" comment="Who to follow?" type="object" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{26} else null">
            <input_param name="class" value="[class.ship]"/>
         </param>
         <param name="IDLE_DOCKING" text="{8888888, 85}" comment="Idle: Docking?" type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{27} else false"/>
         <param name="FIND_STATION" text="- {8888888, 86}" comment="Find a Station?" type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{28} else true"/>
         <param name="WHERE_TO_DOCK" text="- {8888888, 87}" comment="Where to dock?" type="object" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{29} else null">
            <input_param name="class" value="if this.ship.isclass.[class.ship_s, class.ship_xs] then [class.station, class.ship_m, class.ship_l, class.ship_xl] else [class.station, class.ship_l, class.ship_xl]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_997" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="SHOW_MESSAGES" text="{8888888, 90}" comment="Show Messages?" type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{30} else 100">
            <input_param name="truevalue" value="100"/>
         </param>
         <param name="WRITE_TO_LOG" text="{8888888, 91}" comment="Write to logbook?" type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{31} else 100">
            <input_param name="truevalue" value="100"/>
         </param>
         <param name="ADD_ORDER_TAG" text="{8888888, 92}" comment="Add order tag?" type="bool" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{32} else true"/>
         <param name="SAVE_ORDER_SETTINGS" text="{8888888, 93}" comment="Save Order Settings?" type="bool" default="false"/>
         <param name="RESET_ORDER_SETTINGS" text="{8888888, 94}" comment="Reset Order Settings?" type="bool" default="false"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_998" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="DEBUG" text="Debug" type="number" default="if global.$AnotherExplorer_Settings? then global.$AnotherExplorer_Settings.{33} else 0" advanced="true">
            <input_param name="min" value="0"/>
            <input_param name="max" value="100"/>
            <input_param name="step" value="100"/>
         </param>
         <param name="debugchance" text="{1041, 10086}" comment="Print debug output" type="bool" default="0" advanced="true">
            <input_param name="truevalue" value="100"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_999" text="----------------------------------" type="object" default="null" advanced="true">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      </params>
      <skill min="45"/>
      <requires>
         <match shiptype="shiptype.lasertower" negate="true"/>
      </requires>
   </order>

   <interrupts>
      <handler ref="AttackHandler"/>
      <handler ref="MissileLockHandler" />
      <handler ref="ScannedHandler"/>
      <handler ref="InspectedHandler"/>
      <handler ref="FoundAbandonedHandler"/>
      <handler ref="FoundLockboxHandler"/>
      <handler ref="ResupplyHandler"/>
      <handler ref="TideHandler"/>
   </interrupts>

   <init>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <set_value name="$_Ship" exact="this.ship"/>
      <set_value name="$_OrderName" exact="if $_Ship.defaultorder.id == 'Assist' then $_Ship.commander.defaultorder.name else $_Ship.defaultorder.name"/>
      <set_value name="$_AE_Title" exact="'%1 ~ %2 [%3]'.[$_OrderName, $_Ship.knownname, $_Ship.idcode]"/>
      <substitute_text source="$_AE_Title" text="$_AE_Title">
         <replace string="'\033#FFA95908##AE-S\033X ~ '" with="''"/>
         <replace string="'\033#FFA95908##AE-G\033X ~ '" with="''"/>
      </substitute_text>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <add_to_group object="$_Ship" groupname="global.$AE_ShipsGroup"/>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$DEBUG gt 0">
         <set_value name="$_DebugFolderName" exact="'JP_AnotherExplorer.logs'"/>
         <set_value name="$_DebugScriptName" exact="'JP_AnotherExplorerG'"/>
         <set_value name="$_DebugFileName" exact="$_Ship.idcode + '.' + $_DebugScriptName + '.xml.log'"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$SAVE_ORDER_SETTINGS">
         <create_list name="$_SettingsList"/>
         <append_list_elements name="$_SettingsList" other="[
                            $JUMP_GATES, $ACTIVE_JUMP_GATES, $INACTIVE_JUMP_GATES,
                            $ACCELERATORS, $ACTIVE_ACCELERATORS, $INACTIVE_ACCELERATORS,
                            $SUPER_HIGHWAYS, $ACTIVE_SUPER_HIGHWAYS, $INACTIVE_SUPER_HIGHWAYS,
                            $STATIONS, $WHARFS, $SHIPYARDS, $EQUIPMENTDOCKS, $TRADING_STATIONS,
                            $PIRATE_BASES, $RECYCLING_STATIONS, $DEFENCE_STATIONS,
                            $FACTION_HEADQUARTERS, $NON_SPECIAL_STATIONS, $CONSTRUCTION_SITES,
                            $OWNERLESS_SECTORS,
                            $IDLE_TIME, $IDLE_MOVE_TO, $WHERE_TO_MOVE, $IDLE_FOLLOW,
                            $WHO_TO_FOLLOW, $IDLE_DOCKING, $FIND_STATION, $WHERE_TO_DOCK,
                            $SHOW_MESSAGES, $WRITE_TO_LOG, $ADD_ORDER_TAG, $DEBUG]"/>
         <do_if value="global.$AnotherExplorer_Settings?" negate="true">
            <create_list name="global.$AnotherExplorer_Settings"/>
            <append_list_elements name="global.$AnotherExplorer_Settings" other="$_SettingsList"/>
         </do_if>
         <do_else>
            <do_for_each name="$_Setting" in="global.$AnotherExplorer_Settings" counter="$_it">
               <do_if value="$_Setting != $_SettingsList.{$_it}">
                  <set_value name="global.$AnotherExplorer_Settings.{$_it}" exact="$_SettingsList.{$_it}"/>
               </do_if>
            </do_for_each>
         </do_else>
         <remove_value name="$_SettingsList"/>
         <show_help duration="5s" allowclose="true" log="true" custom="'JP_AnotherExplorerG' + ' ~\n' + $_Ship.knownname + ' [' + $_Ship.idcode + ']\n\n' + '{8888888, 95}'"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$RESET_ORDER_SETTINGS">
         <remove_value name="global.$AnotherExplorer_Settings"/>
         <show_help duration="5s" allowclose="true" log="true" custom="'JP_AnotherExplorerG' + ' ~\n' + $_Ship.knownname + ' [' + $_Ship.idcode + ']\n\n' + '{8888888, 96}'"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
   </init>

   <attention min="unknown">
      <actions>

         <do_if value="$SAVE_ORDER_SETTINGS or $RESET_ORDER_SETTINGS">
            <resume label="FINISH_LBL"/>
         </do_if>

         <label name="INIT_LBL"/>
         <wait exact="1ms"/>
         <do_if value="$DEBUG gt 0">
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'" append="false"/>
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ ' + $_DebugScriptName + '.xml ~ SCRIPT_START ~~' + '\n'"/>
         </do_if>

         <do_if value="$ADD_ORDER_TAG">
            <substitute_text source="$_Ship.knownname" text="$_ResultName">
               <replace string="'\033#FFA95908##AE-G\033X ~ '" with="''"/>
            </substitute_text>
            <do_if value="$_Ship.knownname == $_ResultName">
               <set_object_name object="$_Ship" name="'\033#FFA95908##AE-G\033X ~ ' + $_Ship.knownname"/>
            </do_if>
         </do_if>

         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

         <label name="RESUME_LBL"/>
         <wait exact="1ms"/>

         <set_command command="command.investigate"/>
         <set_command_action commandaction="commandaction.calculating"/>

         <create_list name="$_FoundObjects"/>
         <run_script name="'jp.lib.AE.GetObjectsToUncover'">
            <param name="SECTOR"                  value="@global.$AE_SectorBlacklist.{$_Ship}"/>
            <param name="OWNERLESS_SECTORS"       value="$OWNERLESS_SECTORS"/>
            <param name="JUMP_GATES"              value="$JUMP_GATES"/>
            <param name="ACTIVE_JUMP_GATES"       value="$ACTIVE_JUMP_GATES"/>
            <param name="INACTIVE_JUMP_GATES"     value="$INACTIVE_JUMP_GATES"/>
            <param name="ACCELERATORS"            value="$ACCELERATORS"/>
            <param name="ACTIVE_ACCELERATORS"     value="$ACTIVE_ACCELERATORS"/>
            <param name="INACTIVE_ACCELERATORS"   value="$INACTIVE_ACCELERATORS"/>
            <param name="SUPER_HIGHWAYS"          value="$SUPER_HIGHWAYS"/>
            <param name="ACTIVE_SUPER_HIGHWAYS"   value="$ACTIVE_SUPER_HIGHWAYS"/>
            <param name="INACTIVE_SUPER_HIGHWAYS" value="$INACTIVE_SUPER_HIGHWAYS"/>
            <param name="STATIONS"                value="$STATIONS"/>
            <param name="WHARFS"                  value="$WHARFS"/>
            <param name="SHIPYARDS"               value="$SHIPYARDS"/>
            <param name="EQUIPMENTDOCKS"          value="$EQUIPMENTDOCKS"/>
            <param name="TRADING_STATIONS"        value="$TRADING_STATIONS"/>
            <param name="PIRATE_BASES"            value="$PIRATE_BASES"/>
            <param name="RECYCLING_STATIONS"      value="$RECYCLING_STATIONS"/>
            <param name="DEFENCE_STATIONS"        value="$DEFENCE_STATIONS"/>
            <param name="FACTION_HEADQUARTERS"    value="$FACTION_HEADQUARTERS"/>
            <param name="NON_SPECIAL_STATIONS"    value="$NON_SPECIAL_STATIONS"/>
            <param name="CONSTRUCTION_SITES"      value="$CONSTRUCTION_SITES"/>
            <param name="SHOW_MESSAGES"           value="$SHOW_MESSAGES"/>
            <param name="WRITE_TO_LOG"            value="$WRITE_TO_LOG"/>
            <param name="DEBUG"                   value="$DEBUG"/>
            <save_retval name="_FoundObjects" variable="$_FoundObjects"/>
         </run_script>

         <do_if value="$_FoundObjects.count == 0">
            <!-- There are no remaining objects within my defined workspace that need to be uncovered. -->
            <show_help custom="$_AE_Title + '\n\n' + {8888888, 460} + '\n' + {8888888, 461}" position="1" duration="5s" allowclose="true" chance="$SHOW_MESSAGES"/>
            <write_to_logbook title="$_AE_Title" text="{8888888, 460} + '\n' + {8888888, 461}" category="upkeep" interaction="showonmap" object="$_Ship" chance="$WRITE_TO_LOG"/>
            <resume label="IDLE_LBL"/>
         </do_if>

         <do_if value="$_FoundObjects.count gt 0">
            <do_for_each name="$_FoundObject" in="$_FoundObjects">
               <create_order object="$_Ship" id="'JP_lib_AE_UncoverObject'" immediate="true">
                  <param name="OBJECT" value="$_FoundObject"/>
                  <param name="SHOW_MESSAGES"  value="$SHOW_MESSAGES"/>
                  <param name="WRITE_TO_LOG"  value="$WRITE_TO_LOG"/>
                  <param name="DEBUG"  value="$DEBUG"/>
               </create_order>
               <wait exact="1000ms"/>
               <break/>
            </do_for_each>
         </do_if>

         <label name="IDLE_LBL"/>
         <wait exact="1ms"/>
         <!-- I will switch to idle mode and try again later. -->
         <show_help custom="$_AE_Title + '\n\n' + {8888888, 860} + '\n' + {8888888, 55}" position="1" duration="5s" allowclose="true" chance="$SHOW_MESSAGES"/>
         <write_to_logbook title="$_AE_Title" text="{8888888, 860} + '\n' + {8888888, 55}" category="upkeep" interaction="showonmap" object="$_Ship" chance="$WRITE_TO_LOG"/>
         <create_order object="$_Ship" id="'JP_lib_IdleReturnHome'" immediate="true">
            <param name="IDLE_TIME" value="$IDLE_TIME"/>
            <param name="IDLE_MOVE_TO" value="$IDLE_MOVE_TO"/>
            <param name="WHERE_TO_MOVE" value="$WHERE_TO_MOVE"/>
            <param name="START_POSITION" value="$START_POSITION"/>
            <param name="IDLE_FOLLOW" value="$IDLE_FOLLOW"/>
            <param name="WHO_TO_FOLLOW" value="$WHO_TO_FOLLOW"/>
            <param name="IDLE_DOCKING" value="$IDLE_DOCKING"/>
            <param name="FIND_STATION" value="$FIND_STATION"/>
            <param name="WHERE_TO_DOCK" value="$WHERE_TO_DOCK"/>
            <param name="DEBUG_FOLDER_NAME" value="@$_DebugFolderName"/>
            <param name="DEBUG" value="$DEBUG"/>
         </create_order>
         <wait exact="1000ms"/>

         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

         <label name="FINISH_LBL"/>
         <wait exact="1ms"/>
         <do_if value="$DEBUG gt 0">
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ ' + $_DebugScriptName + '.xml ~ SCRIPT_END ~~'"/>
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
         </do_if>

         <set_order_syncpoint_reached order="this.ship.defaultorder"/>
         <create_order object="this.ship" id="'Wait'" default="true"/>

      </actions>
   </attention>

   <on_abort>
   </on_abort>

</aiscript>
