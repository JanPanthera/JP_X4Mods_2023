<?xml version="1.0" encoding="utf-8"?>

<aiscript name="JP_SatelliteDropperG" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="aiscripts.xsd" version="1">

   <order id="JP_SatelliteDropperG" name="_{8888888, 1497}SatelliteDropper - {8888888, 1499}{8888888, 1498}{20001, 901}{8888888, 1499}" category="navigation" infinite="true" allowinloop="false">
      <params>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="SECTOR" text="{20001, 201}" comment="Sector" type="internal" default="null"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_000" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_SATELLITES_MK1" text="{8888888, 1400}" comment="Place mk1 satellites?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{1} else true"/>
         <param name="PLACE_SATELLITES_MK2" text="{8888888, 1401}" comment="Place mk2 satellites?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{2} else false"/>
         <param name="RESTOCK_SATELLITES" text="{8888888, 1402}" comment="Restock satellites?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{3} else false"/>
         <param name="RESTOCK_MAX_AMOUNT" text="{8888888, 1403}" comment="Restock amount?" type="number" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{4} else 10">
            <input_param name="min" value="2"/>
            <input_param name="max" value="(if this.ship.ammostorage.deployable.capacity gt 0 then this.ship.ammostorage.deployable.capacity else 0)"/>
            <input_param name="step" value="2"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_001" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="JUMP_GATES" text="{8888888, 1420}" comment="Jump gates?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{5} else true"/>
         <param name="ACTIVE_JUMP_GATES" text="- {8888888, 1421}" comment="Active jump gates?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{6} else true"/>
         <param name="INACTIVE_JUMP_GATES" text="- {8888888, 1422}" comment="Inactive jump gates?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{7} else true"/>
         <param name="ACCELERATORS" text="{8888888, 1423}" comment="Accelerators?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{8} else true"/>
         <param name="ACTIVE_ACCELERATORS" text="- {8888888, 1424}" comment="Active accelerators?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{9} else true"/>
         <param name="INACTIVE_ACCELERATORS" text="- {8888888, 1425}" comment="Inactive accelerators?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{10} else true"/>
         <param name="SUPER_HIGHWAYS" text="{8888888, 1426}" comment="Superhighways?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{11} else true"/>
         <param name="ACTIVE_SUPER_HIGHWAYS" text="- {8888888, 1427}" comment="Active superhighways?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{12} else true"/>
         <param name="INACTIVE_SUPER_HIGHWAYS" text="- {8888888, 1428}" comment="Inactive superhighways?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{13} else true"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_002" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="STATIONS" text="{8888888, 1430}" comment="Stations?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{14} else true"/>
         <param name="WHARFS" text="- {8888888, 1431}" comment="Wharfs?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{15} else true"/>
         <param name="SHIPYARDS" text="- {8888888, 1432}" comment="Shipyards?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{16} else true"/>
         <param name="EQUIPMENTDOCKS" text="- {8888888, 1433}" comment="Equipment docks?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{17} else true"/>
         <param name="TRADING_STATIONS" text="- {8888888, 1434}" comment="Trading stations?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{18} else true"/>
         <param name="PIRATE_BASES" text="- {8888888, 1435}" comment="Pirate bases?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{19} else true"/>
         <param name="RECYCLING_STATIONS" text="- {8888888, 1436}" comment="Scrap processing facilities?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{20} else true"/>
         <param name="DEFENCE_STATIONS" text="- {8888888, 1437}" comment="Defense stations?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{21} else true"/>
         <param name="FACTION_HEADQUARTERS" text="- {8888888, 1438}" comment="Faction Headquarters?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{22} else true"/>
         <param name="NON_SPECIAL_STATIONS" text="- {8888888, 1439}" comment="Non Special Stations?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{23} else false"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_996" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="IDLE_TIME" text="{8888888, 1480}" comment="Idle Time" type="time" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{24} else 600s">
            <input_param name="min" value="60s"/>
            <input_param name="max" value="3600s"/>
            <input_param name="step" value="10s"/>
         </param>
         <param name="IDLE_MOVE_TO" text="{8888888, 1481}" comment="Idle: Move to?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{25} else true"/>
         <param name="WHERE_TO_MOVE" text="- {8888888, 1482}" comment="Where to move?" type="position" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{26} else [this.ship.sector, this.ship.sector.coreposition]">
            <input_param name="class" value="class.sector"/>
         </param>
         <param name="IDLE_FOLLOW" text="{8888888, 1483}" comment="Idle: Follow?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{27} else false"/>
         <param name="WHO_TO_FOLLOW" text="- {8888888, 1484}" comment="Who to follow?" type="object" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{28} else null">
            <input_param name="class" value="[class.ship]"/>
         </param>
         <param name="IDLE_DOCKING" text="{8888888, 1485}" comment="Idle: Docking?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{29} else false"/>
         <param name="FIND_STATION" text="- {8888888, 1486}" comment="Find a Station?" type="bool" default="true"/>
         <param name="WHERE_TO_DOCK" text="- {8888888, 1487}" comment="Where to dock?" type="object" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{30} else null">
            <input_param name="class" value="if this.ship.isclass.[class.ship_s, class.ship_xs] then [class.station, class.ship_m, class.ship_l, class.ship_xl] else [class.station, class.ship_l, class.ship_xl]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_997" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="ADD_ORDER_TAG" text="{8888888, 1491}" comment="Add order tag?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{31} else true"/>
         <param name="SAVE_ORDER_SETTINGS" text="{8888888, 1493}" comment="Save Order Settings?" type="bool" default="false"/>
         <param name="RESET_ORDER_SETTINGS" text="{8888888, 1494}" comment="Reset Order Settings?" type="bool" default="false"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_998" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="DEBUG" text="Debug" type="number" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{32} else 0" advanced="true">
            <input_param name="step" value="100"/>
            <input_param name="min" value="0"/>
            <input_param name="max" value="100"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_999" text="----------------------------------" type="object" default="null" advanced="true">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      </params>
      <skill min="45"/>
      <requires>
         <match shiptype="shiptype.lasertower" negate="true"/>
      </requires>
   </order>

   <interrupts>
      <handler ref="SectorChangeHandler"/>
      <handler ref="TargetInvalidHandler"/>
      <handler ref="AttackHandler"/>
      <handler ref="MissileLockHandler" />
      <handler ref="ScannedHandler"/>
      <handler ref="InspectedHandler"/>
      <handler ref="FoundAbandonedHandler"/>
      <handler ref="FoundLockboxHandler"/>
      <handler ref="ResupplyHandler"/>
      <handler ref="TideHandler"/>
   </interrupts>

   <init>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <set_value name="$_Ship" exact="this.ship"/>
      <set_value name="$_RadarRange" exact="if $PLACE_SATELLITES_MK1 then 30km else 75km"/>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <set_value name="$_SatelliteMacroMk1" exact="ware.satellite_mk1.objectmacro"/>
      <set_value name="$_SatelliteMacroMk2" exact="ware.satellite_mk2.objectmacro"/>
      <set_value name="$_MultiSatelliteMode" exact="if $PLACE_SATELLITES_MK1 and $PLACE_SATELLITES_MK2 then true else false"/>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <set_value name="global.$SD_SectorBlacklist" exact="table[]" chance="(if not global.$SD_SectorBlacklist? then 100 else 0)"/>
      <set_value name="global.$SD_SectorBlacklist.{$_Ship}" exact="null"/>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$DEBUG gt 0">
         <set_value name="$_DebugFolderName" exact="'JP_SatelliteDropper.logs'"/>
         <set_value name="$_DebugScriptName" exact="'JP_SatelliteDropperG'"/>
         <set_value name="$_DebugFileName" exact="$_Ship.idcode + '.' + $_DebugScriptName + '.xml.log'"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$SAVE_ORDER_SETTINGS">
         <create_list name="$_SettingsList"/>
         <append_list_elements name="$_SettingsList" other="[
                            $PLACE_SATELLITES_MK1, $PLACE_SATELLITES_MK2, $RESTOCK_SATELLITES, $RESTOCK_MAX_AMOUNT,
                            $JUMP_GATES, $ACTIVE_JUMP_GATES, $INACTIVE_JUMP_GATES,
                            $ACCELERATORS, $ACTIVE_ACCELERATORS, $INACTIVE_ACCELERATORS,
                            $SUPER_HIGHWAYS, $ACTIVE_SUPER_HIGHWAYS, $INACTIVE_SUPER_HIGHWAYS,
                            $STATIONS, $WHARFS, $SHIPYARDS, $EQUIPMENTDOCKS, $TRADING_STATIONS,
                            $PIRATE_BASES, $RECYCLING_STATIONS, $DEFENCE_STATIONS, $FACTION_HEADQUARTERS, $NON_SPECIAL_STATIONS,
                            $IDLE_TIME, $IDLE_MOVE_TO, $WHERE_TO_MOVE, $IDLE_FOLLOW, $WHO_TO_FOLLOW, $IDLE_DOCKING, $WHERE_TO_DOCK,
                            $ADD_ORDER_TAG, $DEBUG]"/>
         <do_if value="global.$SatelliteDropper_Settings?" negate="true">
            <create_list name="global.$SatelliteDropper_Settings"/>
            <append_list_elements name="global.$SatelliteDropper_Settings" other="$_SettingsList"/>
         </do_if>
         <do_else>
            <do_for_each name="$_Setting" in="global.$SatelliteDropper_Settings" counter="$_it">
               <do_if value="$_Setting != $_SettingsList.{$_it}">
                  <set_value name="global.$SatelliteDropper_Settings.{$_it}" exact="$_SettingsList.{$_it}"/>
               </do_if>
            </do_for_each>
         </do_else>
         <do_if value="$DEBUG gt 0">
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'"/>
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'global.$SatelliteDropper_Settings'"/>
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'"/>
            <do_for_each name="$_Setting" in="global.$SatelliteDropper_Settings" counter="$_it">
               <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="global.$SatelliteDropper_Settings.{$_it}"/>
            </do_for_each>
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
         </do_if>
         <remove_value name="$_SettingsList"/>
         <show_help duration="5s" allowclose="true" log="true" custom="'JP_SatelliteDropperG' + ' ~\n' + $_Ship.knownname + ' [' + $_Ship.idcode + ']\n\n' + '{8888888, 1495}'"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$RESET_ORDER_SETTINGS">
         <remove_value name="global.$SatelliteDropper_Settings"/>
         <show_help duration="5s" allowclose="true" log="true" custom="'JP_SatelliteDropperG' + ' ~\n' + $_Ship.knownname + ' [' + $_Ship.idcode + ']\n\n' + '{8888888, 1496}'"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$SAVE_ORDER_SETTINGS or $RESET_ORDER_SETTINGS">
         <set_value name="$_Quit" exact="true"/>
      </do_if>
   </init>

   <attention min="unknown">
      <actions>

         <do_if value="$_Quit? and @$_Quit == true">
            <resume label="FINISH_LBL"/>
         </do_if>

         <label name="INIT_LBL"/>
         <wait exact="1s"/>
         <do_if value="$DEBUG gt 0">
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'" append="false"/>
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ ' + $_DebugScriptName + '.xml ~ SCRIPT_START ~~' + '\n'"/>
         </do_if>

         <do_if value="$ADD_ORDER_TAG">
            <substitute_text source="$_Ship.knownname" text="$_ResultName">
               <replace string="'\033#FFA95908##SD-G\033X ~ '" with="''"/>
            </substitute_text>
            <do_if value="$_Ship.knownname == $_ResultName">
               <set_object_name object="$_Ship" name="'\033#FFA95908##SD-G\033X ~ ' + $_Ship.knownname"/>
            </do_if>
         </do_if>

         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

         <label name="RESUME_LBL"/>
         <wait exact="1s"/>

         <do_if value="$_MultiSatelliteMode">
            <do_if value="$_Ship.ammostorage.{$_SatelliteMacroMk1}.count le 0 or $_Ship.ammostorage.{$_SatelliteMacroMk2}.count le 0">
               <set_value name="$_GoRestock" exact="true"/>
            </do_if>
         </do_if>
         <do_else>
            <do_if value="$PLACE_SATELLITES_MK1 and $_Ship.ammostorage.{$_SatelliteMacroMk1}.count le 0">
               <set_value name="$_GoRestock" exact="true"/>
            </do_if>
            <do_if value="$PLACE_SATELLITES_MK2 and $_Ship.ammostorage.{$_SatelliteMacroMk2}.count le 0">
               <set_value name="$_GoRestock" exact="true"/>
            </do_if>
         </do_else>

         <do_if value="@$_GoRestock and not $RESTOCK_SATELLITES">
            <show_help duration="5s" allowclose="true" log="true" custom="'JP_SatelliteDropperG' + ' ~\n' + $_Ship.knownname + ' [' + $_Ship.idcode + ']\n\n' + '{8888888, 1451}'"/>
            <do_if value="$DEBUG gt 0">
               <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'"/>
               <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'{8888888, 1451}'"/>
               <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
            </do_if>
            <resume label="FINISH_LBL"/>
         </do_if>

         <do_if value="@$_GoRestock">
            <run_script name="'jp.lib.SD.RestockSatellites'">
               <param name="PLACE_SATELLITES_MK1" value="$PLACE_SATELLITES_MK1"/>
               <param name="PLACE_SATELLITES_MK2" value="$PLACE_SATELLITES_MK2"/>
               <param name="RESTOCK_MAX_AMOUNT" value="$RESTOCK_MAX_AMOUNT"/>
               <param name="DEBUG" value="$DEBUG"/>
               <save_retval name="RESULT" variable="$_Result"/>
            </run_script>
            <do_if value="$_Result == 'Terminate'">
               <resume label="FINISH_LBL"/>
            </do_if>
         </do_if>

         <create_list name="$_FoundObjects"/>
         <run_script name="'jp.lib.SD.GetObjectsToMark'">
            <param name="SECTOR"                  value="global.$SD_SectorBlacklist.{$_Ship}"/>
            <param name="JUMP_GATES"              value="$JUMP_GATES"/>
            <param name="ACTIVE_JUMP_GATES"       value="$ACTIVE_JUMP_GATES"/>
            <param name="INACTIVE_JUMP_GATES"     value="$INACTIVE_JUMP_GATES"/>
            <param name="ACCELERATORS"            value="$ACCELERATORS"/>
            <param name="ACTIVE_ACCELERATORS"     value="$ACTIVE_ACCELERATORS"/>
            <param name="INACTIVE_ACCELERATORS"   value="$INACTIVE_ACCELERATORS"/>
            <param name="SUPER_HIGHWAYS"          value="$SUPER_HIGHWAYS"/>
            <param name="ACTIVE_SUPER_HIGHWAYS"   value="$ACTIVE_SUPER_HIGHWAYS"/>
            <param name="INACTIVE_SUPER_HIGHWAYS" value="$INACTIVE_SUPER_HIGHWAYS"/>
            <param name="STATIONS"                value="$STATIONS"/>
            <param name="WHARFS"                  value="$WHARFS"/>
            <param name="SHIPYARDS"               value="$SHIPYARDS"/>
            <param name="EQUIPMENTDOCKS"          value="$EQUIPMENTDOCKS"/>
            <param name="TRADING_STATIONS"        value="$TRADING_STATIONS"/>
            <param name="PIRATE_BASES"            value="$PIRATE_BASES"/>
            <param name="RECYCLING_STATIONS"      value="$RECYCLING_STATIONS"/>
            <param name="DEFENCE_STATIONS"        value="$DEFENCE_STATIONS"/>
            <param name="FACTION_HEADQUARTERS"    value="$FACTION_HEADQUARTERS"/>
            <param name="NON_SPECIAL_STATIONS"    value="$NON_SPECIAL_STATIONS"/>
            <param name="DEBUG"                   value="$DEBUG"/>
            <save_retval name="_FoundObjects" variable="$_FoundObjects"/>
            <save_retval name="_Sector"       variable="$_Sector"/>
         </run_script>
         <do_if value="global.$SD_SectorBlacklist.{$_Ship} != null and $_FoundObjects.count == 0">
            <set_value name="global.$SD_SectorBlacklist.{$_Ship}" exact="null"/>
            <resume label="RESUME_LBL"/>
         </do_if>

         <do_if value="$_FoundObjects.count == 0">
            <resume label="IDLE_LBL"/>
         </do_if>

         <do_if value="$_FoundObjects.count gt 0">
            <do_for_each name="$_FoundObject" in="$_FoundObjects">
               <do_if value="$_FoundObject.class == class.gate or $_FoundObject.class == class.highwayentrygate">
                  <create_order object="$_Ship" id="'JP_lib_SD_PlaceSatelliteToObject'" immediate="true">
                     <param name="OBJECT" value="$_FoundObject"/>
                     <param name="SATELLITE_MACRO" value="if $PLACE_SATELLITES_MK2 then $_SatelliteMacroMk2 else $_SatelliteMacroMk1"/>
                     <param name="DEBUG" value="$DEBUG"/>
                  </create_order>
                  <resume label="RESUME_LBL"/>
               </do_if>
            </do_for_each>
            <create_list name="$_SatellitesPositions"/>
            <run_script name="'jp.lib.SD.GetOptimizedSatellitePositions'">
               <param name="OBJECTS" value="$_FoundObjects"/>
               <param name="PLACE_SATELLITES_MK1" value="$PLACE_SATELLITES_MK1"/>
               <param name="PLACE_SATELLITES_MK2" value="$PLACE_SATELLITES_MK2"/>
               <save_retval name="_SatellitesPositions" variable="$_SatellitesPositions"/>
            </run_script>
            <create_order object="$_Ship" id="'JP_lib_SD_PlaceSatelliteToObject'" immediate="true">
               <param name="OBJECT" value="$_SatellitesPositions.{1}.{1}"/>
               <param name="SATELLITE_MACRO" value="$_SatellitesPositions.{1}.{2}"/>
               <param name="DEBUG" value="$DEBUG"/>
            </create_order>
         </do_if>

         <resume label="RESUME_LBL"/>

         <label name="IDLE_LBL"/>
         <wait exact="1s"/>
         <create_order object="$_Ship" id="'JP_lib_SD_IdleReturnHome'">
            <param name="IDLE_TIME" value="$IDLE_TIME"/>
            <param name="IDLE_MOVE_TO" value="$IDLE_MOVE_TO"/>
            <param name="WHERE_TO_MOVE" value="$WHERE_TO_MOVE"/>
            <param name="IDLE_FOLLOW" value="$IDLE_FOLLOW"/>
            <param name="WHO_TO_FOLLOW" value="$WHO_TO_FOLLOW"/>
            <param name="IDLE_DOCKING" value="$IDLE_DOCKING"/>
            <param name="FIND_STATION" value="$FIND_STATION"/>
            <param name="WHERE_TO_DOCK" value="$WHERE_TO_DOCK"/>
            <param name="DEBUG" value="$DEBUG"/>
         </create_order>

         <resume label="RESUME_LBL"/>

         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

         <label name="FINISH_LBL"/>
         <wait exact="1s"/>
         <do_if value="$DEBUG gt 0">
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ ' + $_DebugScriptName + '.xml ~ SCRIPT_END ~~'"/>
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
         </do_if>

         <set_order_syncpoint_reached order="this.ship.defaultorder"/>
         <cancel_order order="this.ship.defaultorder"/>
         <wait exact="1s"/>

      </actions>
   </attention>

   <on_abort>
   </on_abort>

</aiscript>