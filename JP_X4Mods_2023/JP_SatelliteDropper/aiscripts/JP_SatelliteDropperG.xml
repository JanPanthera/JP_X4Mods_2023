<?xml version="1.0" encoding="utf-8"?>

<aiscript name="JP_SatelliteDropperG" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="aiscripts.xsd" version="1">

   <order id="JP_SatelliteDropperG" name="_{8888888, 97}SatelliteDropper - {8888888, 99}{8888888, 98}{20001, 901}{8888888, 99}" description="{8888888, 1471}" category="navigation" infinite="true" allowinloop="false">
      <params>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_001" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_SATELLITES_MK1" text="{8888888, 1400}" comment="Place mk1 satellites?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{1} else 100">
            <input_param name="truevalue" value="100"/>
         </param>
         <param name="PLACE_SATELLITES_MK2" text="{8888888, 1401}" comment="Place mk2 satellites?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{2} else 0">
            <input_param name="truevalue" value="100"/>
         </param>
         <param name="RESTOCK_SATELLITES" text="{8888888, 1402}" comment="Restock satellites?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{3} else 100"/>
         <param name="RESTOCK_AMOUNT_CAP" text="{8888888, 1403}" comment="Restock amount?" type="number" default="[(if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{4} else 10), this.ship.ammostorage.deployable.capacity].min">
            <input_param name="min" value="2"/>
            <input_param name="max" value="this.ship.ammostorage.deployable.capacity"/>
            <input_param name="step" value="2"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_002" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="JUMP_GATES"              text="{8888888, 1}"   comment="Jump Gates?"             type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{5} else true"/>
         <param name="ACTIVE_JUMP_GATES"       text="- {8888888, 2}" comment="Active Jump Gates?"      type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{6} else true"/>
         <param name="INACTIVE_JUMP_GATES"     text="- {8888888, 3}" comment="Inactive Jump Gates?"    type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{7} else true"/>
         <param name="ACCELERATORS"            text="{8888888, 4}"   comment="Accelerators?"           type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{8} else true"/>
         <param name="ACTIVE_ACCELERATORS"     text="- {8888888, 5}" comment="Active Accelerators?"    type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{9} else true"/>
         <param name="INACTIVE_ACCELERATORS"   text="- {8888888, 6}" comment="Inactive Accelerators?"  type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{10} else true"/>
         <param name="SUPER_HIGHWAYS"          text="{8888888, 7}"   comment="Superhighways?"          type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{11} else true"/>
         <param name="ACTIVE_SUPER_HIGHWAYS"   text="- {8888888, 8}" comment="Active Superhighways?"   type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{12} else true"/>
         <param name="INACTIVE_SUPER_HIGHWAYS" text="- {8888888, 9}" comment="Inactive Superhighways?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{13} else true"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_003" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="STATIONS"             text="{8888888, 10}"   comment="Stations?"                    type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{14} else true"/>
         <param name="WHARFS"               text="- {8888888, 11}" comment="Wharfs?"                      type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{15} else true"/>
         <param name="SHIPYARDS"            text="- {8888888, 12}" comment="Shipyards?"                   type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{16} else true"/>
         <param name="EQUIPMENTDOCKS"       text="- {8888888, 13}" comment="Equipment docks?"             type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{17} else true"/>
         <param name="TRADING_STATIONS"     text="- {8888888, 14}" comment="Trading stations?"            type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{18} else true"/>
         <param name="PIRATE_BASES"         text="- {8888888, 15}" comment="Pirate bases?"                type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{19} else true"/>
         <param name="RECYCLING_STATIONS"   text="- {8888888, 16}" comment="Scrap processing facilities?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{20} else true"/>
         <param name="DEFENCE_STATIONS"     text="- {8888888, 17}" comment="Defense stations?"            type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{21} else true"/>
         <param name="FACTION_HEADQUARTERS" text="- {8888888, 18}" comment="Faction Headquarters?"        type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{22} else true"/>
         <param name="NON_SPECIAL_STATIONS" text="- {8888888, 19}" comment="Factories?"                   type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{23} else true"/>
         <param name="CONSTRUCTION_SITES"   text="- {8888888, 20}" comment="Construction Sites?"          type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{24} else true"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_995" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="OWNERLESS_SECTORS" text="{8888888, 30}" comment="Ownerless Sectors?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{25} else true"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_996" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="IDLE_TIME" text="{8888888, 80}" comment="Idle Time" type="time" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{26} else 600s">
            <input_param name="min" value="60s"/>
            <input_param name="max" value="3600s"/>
            <input_param name="step" value="10s"/>
         </param>
         <param name="IDLE_MOVE_TO" text="{8888888, 81}" comment="Idle: Move to?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{27} else true"/>
         <param name="WHERE_TO_MOVE" text="- {8888888, 82}" comment="Where to move?" type="position" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{28} else [this.ship.sector, this.ship.sector.coreposition]">
            <input_param name="class" value="class.sector"/>
         </param>
         <param name="START_POSITION" type="internal" default="[this.ship.sector, this.ship.position]"/>
         <param name="IDLE_FOLLOW" text="{8888888, 83}" comment="Idle: Follow?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{29} else false"/>
         <param name="WHO_TO_FOLLOW" text="- {8888888, 84}" comment="Who to follow?" type="object" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{30} else null">
            <input_param name="class" value="[class.ship]"/>
         </param>
         <param name="IDLE_DOCKING" text="{8888888, 85}" comment="Idle: Docking?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{31} else false"/>
         <param name="FIND_STATION" text="- {8888888, 86}" comment="Find a Station?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{32} else true"/>
         <param name="WHERE_TO_DOCK" text="- {8888888, 87}" comment="Where to dock?" type="object" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{33} else null">
            <input_param name="class" value="if this.ship.isclass.[class.ship_s, class.ship_xs] then [class.station, class.ship_m, class.ship_l, class.ship_xl] else [class.station, class.ship_l, class.ship_xl]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_997" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="SHOW_MESSAGES" text="{8888888, 90}" comment="Show Messages?" type="number" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{34} else 1">
            <input_param name="min" value="0"/>
            <input_param name="max" value="3"/>
            <input_param name="step" value="1"/>
         </param>
         <param name="WRITE_TO_LOG" text="{8888888, 91}" comment="Write to logbook?" type="number" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{35} else 3">
            <input_param name="min" value="0"/>
            <input_param name="max" value="3"/>
            <input_param name="step" value="1"/>
         </param>
         <param name="ADD_ORDER_TAG" text="{8888888, 92}" comment="Add order tag?" type="bool" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{36} else true"/>
         <param name="SAVE_ORDER_SETTINGS" text="{8888888, 93}" comment="Save Order Settings?" type="bool" default="false"/>
         <param name="RESET_ORDER_SETTINGS" text="{8888888, 94}" comment="Reset Order Settings?" type="bool" default="false"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_998" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="DEBUG_FOLDER_NAME" type="internal" default="'JP_SatelliteDropper.logs'"/>
         <param name="DEBUG" text="Debug" type="number" default="if global.$SatelliteDropper_Settings? then global.$SatelliteDropper_Settings.{37} else 0" advanced="true">
            <input_param name="min" value="0"/>
            <input_param name="max" value="100"/>
            <input_param name="step" value="100"/>
         </param>
         <param name="debugchance" type="internal" default="0"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_999" text="----------------------------------" type="object" default="null" advanced="true">
            <input_param name="class" value="[]"/>
            <input_param name="playerreadonly" value="true"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      </params>
      <skill min="45"/>
      <requires>
         <match shiptype="shiptype.lasertower" negate="true"/>
      </requires>
   </order>

   <interrupts>
      <handler ref="AttackHandler"/>
      <handler ref="MissileLockHandler" />
      <handler ref="ScannedHandler"/>
      <handler ref="InspectedHandler"/>
      <handler ref="FoundAbandonedHandler"/>
      <handler ref="FoundLockboxHandler"/>
      <handler ref="ResupplyHandler"/>
      <handler ref="TideHandler"/>
   </interrupts>

   <init>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <set_value name="$_Ship" exact="this.ship"/>
      <set_value name="$_DefaultOrderParamRef" exact="if $_Ship.defaultorder.id == 'Assist' then $_Ship.commander.defaultorder else $_Ship.defaultorder"/>
      <set_value name="$_SD_Title" exact="'%1 ~ %2 [%3]'.[$_DefaultOrderParamRef.name, $_Ship.knownname, $_Ship.idcode]"/>
      <substitute_text source="$_SD_Title" text="$_SD_Title">
         <replace string="'\033#FFA95908##SD-S\033X ~ '" with="''"/>
         <replace string="'\033#FFA95908##SD-G\033X ~ '" with="''"/>
      </substitute_text>
      <set_value name="$_RadarRange" exact="if $PLACE_SATELLITES_MK1 then 30km else 75km"/>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <add_to_group object="$_Ship" groupname="global.$SD_ShipsGroup"/>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <set_value name="$_SatelliteMacroMk1" exact="ware.satellite_mk1.objectmacro"/>
      <set_value name="$_SatelliteMacroMk2" exact="ware.satellite_mk2.objectmacro"/>
      <set_value name="$_MultiSatMode" exact="if $_SatelliteMacroMk1 and $_SatelliteMacroMk2 then 100 else 0"/>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$DEBUG gt 0">
         <set_value name="$_DebugFolderName" exact="$DEBUG_FOLDER_NAME"/>
         <set_value name="$_DebugScriptName" exact="'JP_SatelliteDropperG'"/>
         <set_value name="$_DebugFileName" exact="$_Ship.idcode + '.' + $_DebugScriptName + '.xml.log'"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$SAVE_ORDER_SETTINGS">
         <create_list name="$_SettingsList"/>
         <append_list_elements name="$_SettingsList" other="[
                            $PLACE_SATELLITES_MK1, $PLACE_SATELLITES_MK2, $RESTOCK_SATELLITES, $RESTOCK_AMOUNT_CAP,
                            $JUMP_GATES, $ACTIVE_JUMP_GATES, $INACTIVE_JUMP_GATES,
                            $ACCELERATORS, $ACTIVE_ACCELERATORS, $INACTIVE_ACCELERATORS,
                            $SUPER_HIGHWAYS, $ACTIVE_SUPER_HIGHWAYS, $INACTIVE_SUPER_HIGHWAYS,
                            $STATIONS, $WHARFS, $SHIPYARDS, $EQUIPMENTDOCKS, $TRADING_STATIONS,
                            $PIRATE_BASES, $RECYCLING_STATIONS, $DEFENCE_STATIONS, $FACTION_HEADQUARTERS,
                            $NON_SPECIAL_STATIONS, $CONSTRUCTION_SITES, $OWNERLESS_SECTORS,
                            $IDLE_TIME, $IDLE_MOVE_TO, $WHERE_TO_MOVE, $IDLE_FOLLOW,
                            $WHO_TO_FOLLOW, $IDLE_DOCKING, $FIND_STATION, $WHERE_TO_DOCK,
                            $SHOW_MESSAGES, $WRITE_TO_LOG, $ADD_ORDER_TAG, $DEBUG]"/>
         <do_if value="global.$SatelliteDropper_Settings?" negate="true">
            <create_list name="global.$SatelliteDropper_Settings"/>
            <append_list_elements name="global.$SatelliteDropper_Settings" other="$_SettingsList"/>
         </do_if>
         <do_else>
            <do_for_each name="$_Setting" in="global.$SatelliteDropper_Settings" counter="$_it">
               <do_if value="$_Setting != $_SettingsList.{$_it}">
                  <set_value name="global.$SatelliteDropper_Settings.{$_it}" exact="$_SettingsList.{$_it}"/>
               </do_if>
            </do_for_each>
         </do_else>
         <remove_value name="$_SettingsList"/>
         <show_help custom="$_SD_Title + '\n\n' + {8888888, 95}" position="1" allowclose="true" chance="if $SHOW_MESSAGES ge 1 then 100 else 0"/>
         <write_to_logbook title="$_SD_Title" text="{8888888, 95}" category="upkeep" interaction="showonmap" object="$_Ship" chance="if $WRITE_TO_LOG ge 1 then 100 else 0"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$RESET_ORDER_SETTINGS">
         <remove_value name="global.$SatelliteDropper_Settings"/>
         <show_help custom="$_SD_Title + '\n\n' + {8888888, 96}" position="1" allowclose="true" chance="if $SHOW_MESSAGES ge 1 then 100 else 0"/>
         <write_to_logbook title="$_SD_Title" text="{8888888, 96}" category="upkeep" interaction="showonmap" object="$_Ship" chance="if $WRITE_TO_LOG ge 1 then 100 else 0"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
   </init>

   <attention min="unknown">
      <actions>

         <do_if value="$SAVE_ORDER_SETTINGS or $RESET_ORDER_SETTINGS">
            <resume label="FINISH_LBL"/>
         </do_if>

         <label name="INIT_LBL"/>
         <wait exact="1ms"/>
         <do_if value="$DEBUG gt 0">
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'" append="false"/>
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ ' + $_DebugScriptName + '.xml ~ SCRIPT_START ~~' + '\n'"/>
         </do_if>

         <do_if value="$ADD_ORDER_TAG">
            <substitute_text source="$_Ship.knownname" text="$_ResultName">
               <replace string="'\033#FFA95908##SD-G\033X ~ '" with="''"/>
            </substitute_text>
            <do_if value="$_Ship.knownname == $_ResultName">
               <set_object_name object="$_Ship" name="'\033#FFA95908##SD-G\033X ~ ' + $_Ship.knownname"/>
            </do_if>
         </do_if>

         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

         <label name="RESUME_LBL"/>
         <wait exact="1ms"/>

         <set_command command="command.investigate"/>
         <set_command_action commandaction="commandaction.calculating"/>

         <set_value name="$_GoRestock" exact="false"/>
         <do_if value="($_MultiSatMode and ($_Ship.ammostorage.{$_SatelliteMacroMk1}.count == 0 or $_Ship.ammostorage.{$_SatelliteMacroMk2}.count == 0)) or
                       ($PLACE_SATELLITES_MK1 and $_Ship.ammostorage.{$_SatelliteMacroMk1}.count == 0) or
                       ($PLACE_SATELLITES_MK2 and $_Ship.ammostorage.{$_SatelliteMacroMk2}.count == 0)">
            <set_value name="$_GoRestock" exact="true"/>
         </do_if>

         <do_if value="$_GoRestock">
            <do_if value="not $RESTOCK_SATELLITES">
               <!-- There are no or not enough satellites available, and the restocking of satellites is deactivated.\nWhen using both types of satellites, at least one of each type must be available. -->
               <show_help custom="$_SD_Title + '\n\n' + {8888888, 1460} + '\n' + {8888888, 1461}" position="1" allowclose="true" chance="if $SHOW_MESSAGES ge 1 then 100 else 0"/>
               <write_to_logbook title="$_SD_Title" text="{8888888, 1460} + '\n' + {8888888, 1461}" category="upkeep" interaction="showonmap" object="$_Ship" chance="if $WRITE_TO_LOG ge 1 then 100 else 0"/>
               <resume label="IDLE_LBL"/>
            </do_if>
            <set_value name="$_RestockAmount" exact="($RESTOCK_AMOUNT_CAP / 2)i" chance="$_MultiSatMode"/>
            <set_value name="$_RestockEquipmentTable" exact="table[]"/>
            <set_value name="$_RestockEquipmentTable.{$_SatelliteMacroMk1}" exact="$_RestockAmount" chance="$PLACE_SATELLITES_MK1"/>
            <set_value name="$_RestockEquipmentTable.{$_SatelliteMacroMk2}" exact="$_RestockAmount" chance="$PLACE_SATELLITES_MK2"/>
            <run_script name="'jp.lib.RestockDeployables'" result="$_Result">
               <param name="DEPLOYABLE_TABLE" value="$_RestockEquipmentTable"/>
               <param name="FORCE_RERSTOCK" value="true"/>
               <param name="RESTOCK_TITLE" value="$_SD_Title"/>
            </run_script>
            <!-- The restock equipment order has failed. More detail is available in the logbook. -->
            <do_if value="$_Result == 'Failed'">
               <show_help custom="$_ASE_Title + '\n\n' + {8888888, 1460} + '\n' +  {8888888, 54}" position="1" allowclose="true" chance="if $SHOW_MESSAGES ge 1 then 100 else 0"/>
               <write_to_logbook title="$_ASE_Title" text="{8888888, 1460} + '\n' + {8888888, 54}" category="upkeep" interaction="showonmap" object="$_Ship" chance="if $WRITE_TO_LOG ge 1 then 100 else 0"/>
               <resume label="IDLE_LBL"/>
            </do_if>
         </do_if>

         <create_list name="$_FoundObjects"/>
         <run_script name="'jp.lib.SD.GetObjectsToMark'" result="$_FoundObjects">
            <param name="SECTOR"                  value="@global.$SD_SectorBlacklist.{$_Ship}"/>
            <param name="OWNERLESS_SECTORS"       value="$OWNERLESS_SECTORS"/>
            <param name="JUMP_GATES"              value="$JUMP_GATES"/>
            <param name="ACTIVE_JUMP_GATES"       value="$ACTIVE_JUMP_GATES"/>
            <param name="INACTIVE_JUMP_GATES"     value="$INACTIVE_JUMP_GATES"/>
            <param name="ACCELERATORS"            value="$ACCELERATORS"/>
            <param name="ACTIVE_ACCELERATORS"     value="$ACTIVE_ACCELERATORS"/>
            <param name="INACTIVE_ACCELERATORS"   value="$INACTIVE_ACCELERATORS"/>
            <param name="SUPER_HIGHWAYS"          value="$SUPER_HIGHWAYS"/>
            <param name="ACTIVE_SUPER_HIGHWAYS"   value="$ACTIVE_SUPER_HIGHWAYS"/>
            <param name="INACTIVE_SUPER_HIGHWAYS" value="$INACTIVE_SUPER_HIGHWAYS"/>
            <param name="STATIONS"                value="$STATIONS"/>
            <param name="WHARFS"                  value="$WHARFS"/>
            <param name="SHIPYARDS"               value="$SHIPYARDS"/>
            <param name="EQUIPMENTDOCKS"          value="$EQUIPMENTDOCKS"/>
            <param name="TRADING_STATIONS"        value="$TRADING_STATIONS"/>
            <param name="PIRATE_BASES"            value="$PIRATE_BASES"/>
            <param name="RECYCLING_STATIONS"      value="$RECYCLING_STATIONS"/>
            <param name="DEFENCE_STATIONS"        value="$DEFENCE_STATIONS"/>
            <param name="FACTION_HEADQUARTERS"    value="$FACTION_HEADQUARTERS"/>
            <param name="NON_SPECIAL_STATIONS"    value="$NON_SPECIAL_STATIONS"/>
            <param name="CONSTRUCTION_SITES"      value="$CONSTRUCTION_SITES"/>
         </run_script>

         <do_if value="$_FoundObjects.count == 0">
            <!-- There are no remaining structures left where we need to deploy a satellite. -->
            <show_help custom="$_SD_Title + '\n\n' + {8888888, 1460} + '\n' + {8888888, 1462}" position="1" allowclose="true" chance="if $SHOW_MESSAGES ge 1 then 100 else 0"/>
            <write_to_logbook title="$_SD_Title" text="{8888888, 1460} + '\n' + {8888888, 1462}" category="upkeep" interaction="showonmap" object="$_Ship" chance="if $WRITE_TO_LOG ge 1 then 100 else 0"/>
            <resume label="IDLE_LBL"/>
         </do_if>

         <do_for_each name="$_FoundObject" in="$_FoundObjects">
            <do_if value="$_FoundObject.class == class.gate or $_FoundObject.class == class.highwayentrygate">
               <create_order object="$_Ship" id="'JP_lib_SD_PlaceSatelliteToObject'" immediate="true">
                  <param name="OBJECT" value="$_FoundObject"/>
                  <param name="SATELLITE_MACRO" value="if $PLACE_SATELLITES_MK2 then $_SatelliteMacroMk2 else $_SatelliteMacroMk1"/>
               </create_order>
               <wait exact="1000ms"/>
            </do_if>
         </do_for_each>
         <run_script name="'jp.lib.SD.GetOptimizedSatellitePositions'" result="$_FoundObjects">
            <param name="OBJECTS" value="$_FoundObjects"/>
            <param name="PLACE_SATELLITES_MK1" value="$PLACE_SATELLITES_MK1"/>
            <param name="PLACE_SATELLITES_MK2" value="$PLACE_SATELLITES_MK2"/>
         </run_script>
         <create_order object="$_Ship" id="'JP_lib_SD_PlaceSatelliteToObject'" immediate="true">
            <param name="OBJECT" value="$_FoundObjects.{1}"/>
            <param name="SATELLITE_MACRO" value="$_FoundObjects.{2}"/>
         </create_order>
         <wait exact="1000ms"/>

         <label name="IDLE_LBL"/>
         <wait exact="1ms"/>
         <!-- I will switch to idle mode and try again later. -->
         <show_help custom="$_SD_Title + '\n\n' + {8888888, 1460} + '\n' + {8888888, 55}" position="1" allowclose="true" chance="if $SHOW_MESSAGES ge 1 then 100 else 0"/>
         <write_to_logbook title="$_SD_Title" text="{8888888, 1460} + '\n' + {8888888, 55}" category="upkeep" interaction="showonmap" object="$_Ship" chance="if $WRITE_TO_LOG ge 1 then 100 else 0"/>
         <create_order object="$_Ship" id="'JP_lib_IdleReturnHome'" immediate="true">
            <param name="IDLE_TIME" value="$IDLE_TIME"/>
            <param name="IDLE_MOVE_TO" value="$IDLE_MOVE_TO"/>
            <param name="WHERE_TO_MOVE" value="$WHERE_TO_MOVE"/>
            <param name="START_POSITION" value="$START_POSITION"/>
            <param name="IDLE_FOLLOW" value="$IDLE_FOLLOW"/>
            <param name="WHO_TO_FOLLOW" value="$WHO_TO_FOLLOW"/>
            <param name="IDLE_DOCKING" value="$IDLE_DOCKING"/>
            <param name="FIND_STATION" value="$FIND_STATION"/>
            <param name="WHERE_TO_DOCK" value="$WHERE_TO_DOCK"/>
         </create_order>
         <wait exact="1000ms"/>

         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

         <label name="FINISH_LBL"/>
         <wait exact="1ms"/>
         <do_if value="$DEBUG gt 0">
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ ' + $_DebugScriptName + '.xml ~ SCRIPT_END ~~'"/>
            <debug_to_file directory="$_DebugFolderName" name="$_DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
         </do_if>

         <set_order_syncpoint_reached order="this.ship.defaultorder"/>
         <create_order object="this.ship" id="'Wait'" default="true"/>

      </actions>
   </attention>

   <on_abort>
   </on_abort>

</aiscript>