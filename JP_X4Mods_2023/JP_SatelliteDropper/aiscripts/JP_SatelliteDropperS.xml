<?xml version="1.0" encoding="utf-8"?>

<aiscript name="JP_SatelliteDropperS" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="aiscripts.xsd" version="1">

   <order id="JP_SatelliteDropperS" name="_{8888888, 1497}SatelliteDropper - {8888888, 1499}{8888888, 1498}{20001, 201}{8888888, 1499}" category="navigation" infinite="true" allowinloop="false">
      <params>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="SECTOR" text="{20001, 201}" comment="Sector" type="object" default="this.ship.sector">
            <input_param name="class" value="[class.sector]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_000" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="RESTOCK_SATELLITES_MK1" text="{8888888, 1400}" comment="Restock mk1 satellites?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{1} else true"/>
         <param name="RESTOCK_SATELLITES_MK2" text="{8888888, 1401}" comment="Restock mk2 satellites?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{2} else false"/>
         <param name="RESTOCK_AMOUNT" text="{8888888, 1402}" comment="Restock amount?" type="number" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{3} else 10">
            <input_param name="min" value="0"/>
            <input_param name="max" value="(if this.ship.ammostorage.deployable.capacity gt 0 then this.ship.ammostorage.deployable.capacity else 0)"/>
            <input_param name="step" value="1"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_001" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="JUMP_GATES" text="{8888888, 1420}" comment="Jump gates?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{4} else true"/>
         <param name="ACTIVE_JUMP_GATES" text="- {8888888, 1421}" comment="Active jump gates?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{5} else true"/>
         <param name="INACTIVE_JUMP_GATES" text="- {8888888, 1422}" comment="Inactive jump gates?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{6} else true"/>
         <param name="ACCELERATORS" text="{8888888, 1423}" comment="Accelerators?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{7} else true"/>
         <param name="ACTIVE_ACCELERATORS" text="- {8888888, 1424}" comment="Active accelerators?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{8} else true"/>
         <param name="INACTIVE_ACCELERATORS" text="- {8888888, 1425}" comment="Inactive accelerators?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{9} else true"/>
         <param name="SUPER_HIGHWAYS" text="{8888888, 1426}" comment="Superhighways?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{10} else true"/>
         <param name="ACTIVE_SUPER_HIGHWAYS" text="- {8888888, 1427}" comment="Active superhighways?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{11} else true"/>
         <param name="INACTIVE_SUPER_HIGHWAYS" text="- {8888888, 1428}" comment="Inactive superhighways?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{12} else true"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_002" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="STATIONS" text="{8888888, 1430}" comment="Stations?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{13} else true"/>
         <param name="WHARFS" text="- {8888888, 1431}" comment="Wharfs?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{14} else true"/>
         <param name="SHIPYARDS" text="- {8888888, 1432}" comment="Shipyards?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{15} else true"/>
         <param name="EQUIPMENTDOCKS" text="- {8888888, 1433}" comment="Equipment docks?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{16} else true"/>
         <param name="TRADING_STATIONS" text="- {8888888, 1434}" comment="Trading stations?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{17} else true"/>
         <param name="PIRATE_BASES" text="- {8888888, 1435}" comment="Pirate bases?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{18} else true"/>
         <param name="RECYCLING_STATIONS" text="- {8888888, 1436}" comment="Scrap processing facilities?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{19} else true"/>
         <param name="DEFENCE_STATIONS" text="- {8888888, 1437}" comment="Defense stations?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{20} else true"/>
         <param name="FACTION_HEADQUARTERS" text="- {8888888, 1438}" comment="Faction Headquarters?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{21} else true"/>
         <param name="NON_SPECIAL_STATIONS" text="- {8888888, 1439}" comment="Non Special Stations?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{22} else false"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_996" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="IDLE_TIME" text="{8888888, 1481}" comment="Idle Time" type="time" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{23} else 600s">
            <input_param name="min" value="60s"/>
            <input_param name="max" value="3600s"/>
            <input_param name="step" value="10s"/>
         </param>
         <param name="IDLE_MOVE_TO" text="{8888888, 1488}" comment="Idle: Move to?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{24} else true"/>
         <param name="WHERE_TO_MOVE" text="- {8888888, 1489}" comment="Where to move?" type="position" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{25} else [this.ship.sector, position.[0, 0, 0]]">
            <input_param name="class" value="class.sector"/>
         </param>
         <param name="IDLE_FOLLOW" text="{8888888, 1482}" comment="Idle: Follow?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{26} else false"/>
         <param name="WHO_TO_FOLLOW" text="- {8888888, 1483}" comment="Who to follow?" type="object" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{27} else null">
            <input_param name="class" value="[class.ship]"/>
            <input_param name="owner" value="this.ship.owner"/>
         </param>
         <param name="IDLE_DOCKING" text="{8888888, 1484}" comment="Idle: Docking?" type="bool" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{28} else false"/>
         <param name="WHERE_TO_DOCK" text="- {8888888, 1485}" comment="Where to dock?" type="object" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{29} else null">
            <input_param name="class" value="if this.ship.isclass.[class.ship_s, class.ship_xs] then [class.station, class.ship_m, class.ship_l, class.ship_xl] else [class.station, class.ship_l, class.ship_xl]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_997" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="ADD_ORDER_TAG" text="{8888888, 1491}" comment="Add order tag?" type="bool" default="true"/>
         <param name="ORDER_TAG_POSITION" text="{8888888, 1492}" comment="Order tag position" type="number" default="1">
            <input_param name="min" value="0"/>
            <input_param name="max" value="1"/>
            <input_param name="step" value="1"/>
         </param>
         <param name="SAVE_ORDER_SETTINGS" text="{8888888, 1493}" comment="Save Order Settings?" type="bool" default="false"/>
         <param name="RESET_ORDER_SETTINGS" text="{8888888, 1494}" comment="Reset Order Settings?" type="bool" default="false"/>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_998" text="----------------------------------" type="object" default="null">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="DEBUG" text="Debug" type="number" default="if global.$SatelliteDropperS_Settings? then global.$SatelliteDropperS_Settings.{30} else 100" advanced="true">
            <input_param name="step" value="100"/>
            <input_param name="min" value="0"/>
            <input_param name="max" value="100"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
         <param name="PLACE_HOLDER_999" text="----------------------------------" type="object" default="null" advanced="true">
            <input_param name="class" value="[]"/>
         </param>
         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      </params>
      <skill min="15"/>
      <requires>
         <match shiptype="shiptype.lasertower" negate="true"/>
      </requires>
      <location object="@$LOCATION.{1}" position="@$LOCATION.{2}" condition="@$LOCATION.{1} and @$LOCATION.{2}"/>
   </order>

   <interrupts>
      <handler ref="SectorChangeHandler"/>
      <handler ref="TargetInvalidHandler"/>
      <handler ref="AttackHandler"/>
      <handler ref="MissileLockHandler" />
      <handler ref="ScannedHandler"/>
      <handler ref="InspectedHandler"/>
      <handler ref="FoundAbandonedHandler"/>
      <handler ref="FoundLockboxHandler"/>
      <handler ref="ResupplyHandler"/>
      <handler ref="TideHandler"/>
   </interrupts>

   <init>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <set_value name="$Ship" exact="this.ship"/>
      <set_value name="$LOCATION" exact="[$SECTOR, position.[0, 0, 0]]"/>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$DEBUG">
         <set_value name="$DebugFolderName" exact="'JP_SatelliteDropper.logs'"/>
         <set_value name="$DebugScriptName" exact="'JP_SatelliteDropperS'"/>
         <set_value name="$DebugFileName" exact="$Ship.idcode + '.' + $DebugScriptName + '.xml.log'"/>
         <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'" append="false"/>
         <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ ' + $DebugScriptName + '.xml ~ Started ~~' + '\n'"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$SAVE_ORDER_SETTINGS">
         <create_list name="$_SettingsList"/>
         <append_list_elements name="$_SettingsList" other="[
                            $RESTOCK_SATELLITES_MK1, $RESTOCK_SATELLITES_MK2, $RESTOCK_AMOUNT,
                            $JUMP_GATES, $ACTIVE_JUMP_GATES, $INACTIVE_JUMP_GATES,
                            $ACCELERATORS, $ACTIVE_ACCELERATORS, $INACTIVE_ACCELERATORS,
                            $SUPER_HIGHWAYS, $ACTIVE_SUPER_HIGHWAYS, $INACTIVE_SUPER_HIGHWAYS,
                            $STATIONS, $WHARFS, $SHIPYARDS, $EQUIPMENTDOCKS, $TRADING_STATIONS,
                            $PIRATE_BASES, $RECYCLING_STATIONS, $DEFENCE_STATIONS, $FACTION_HEADQUARTERS, $NON_SPECIAL_STATIONS,
                            $IDLE_TIME, $IDLE_MOVE_TO, $WHERE_TO_MOVE, $IDLE_FOLLOW, $WHO_TO_FOLLOW, $IDLE_DOCKING, $WHERE_TO_DOCK,
                            $DEBUG]"/>
         <do_if value="global.$SatelliteDropperS_Settings?" negate="true">
            <create_list name="global.$SatelliteDropperS_Settings"/>
            <append_list_elements name="global.$SatelliteDropperS_Settings" other="$_SettingsList"/>
         </do_if>
         <do_else>
            <do_for_each name="$_Setting" in="global.$SatelliteDropperS_Settings" counter="$_it">
               <do_if value="$_Setting != $_SettingsList.{$_it}">
                  <set_value name="global.$SatelliteDropperS_Settings.{$_it}" exact="$_SettingsList.{$_it}"/>
               </do_if>
            </do_for_each>
         </do_else>
         <do_if value="$DEBUG gt 0">
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'"/>
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'global.$SatelliteDropperS_Settings'"/>
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'"/>
            <do_for_each name="$_Setting" in="global.$SatelliteDropperS_Settings" counter="$_it">
               <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="global.$SatelliteDropperS_Settings.{$_it}"/>
            </do_for_each>
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
         </do_if>
         <remove_value name="$_SettingsList"/>
         <show_help duration="5s" allowclose="true" log="true" custom="'JP_SatelliteDropperS' + ' ~\n' + $Ship.knownname + ' [' + $Ship.idcode + ']\n\n' + '{8888888, 1495}'"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <do_if value="$RESET_ORDER_SETTINGS">
         <do_if value="global.$SatelliteDropperS_Settings?">
            <remove_value name="global.$SatelliteDropperS_Settings"/>
         </do_if>
         <show_help duration="5s" allowclose="true" log="true" custom="'JP_SatelliteDropperS' + ' ~\n' + $Ship.knownname + ' [' + $Ship.idcode + ']\n\n' + '{8888888, 1496}'"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <!-- Remove later? -->
      <do_if value="$SAVE_ORDER_SETTINGS or $RESET_ORDER_SETTINGS">
         <set_value name="$Quit" exact="true"/>
      </do_if>
      <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
   </init>

   <attention min="unknown">
      <actions>

         <do_if value="$Quit?">
            <resume label="FINISH_LBL"/>
         </do_if>

         <label name="INIT_LBL"/>
         <wait min="500ms" max="1000ms"/>

         <!--<set_value name="$_OrderTag" exact="if $ORDER_TAG_POSITION == 0 then '#SD-S ' else ' #SD-S'"/>
         <substitute_text source="$Ship.knownname" text="$_ResultName">
            <replace string="('\033#FFA95908#' + $_OrderTag + '\033X')" with="''"/>
         </substitute_text>
         <do_if value="$Ship.knownname == $_ResultName">
            <set_object_name object="$Ship" name="(if $ORDER_TAG_POSITION == 0 then ('\033#FFA95908#' + $_OrderTag + '\033X') else '') + $Ship.knownname + (if $ORDER_TAG_POSITION == 1 then ('\033#FFA95908#' + $_OrderTag + '\033X') else '')"/>
         </do_if>
         <remove_value name="$_OrderTag"/>-->

         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

         <label name="RESUME_LBL"/>
         <wait min="500ms" max="1000ms"/>

         <!-- just for the ui -->
         <set_command command="command.scan"/>
         <set_command_action commandaction="commandaction.calculating"/>

         <!-- If allowed we first check if we need to restock -->
         <do_if value="$RESTOCK_SATELLITES_MK1 or $RESTOCK_SATELLITES_MK2">
            <include_interrupt_actions ref="JP_Lib_RestockSatellites_SD"/>
         </do_if>

         <!-- just for the ui -->
         <set_command command="command.scan"/>
         <set_command_action commandaction="commandaction.calculating"/>

         <!-- Return value ~~ $_FoundObjects -->
         <include_interrupt_actions ref="JP_Lib_GetNearestObjectToMark_SD"/>

         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

         <!--<set_value name="$_RadarRange" exact="if $RESTOCK_SATELLITES_MK1 then 30km else 75km"/>
         <create_list name="$_Objects"/>
         <do_for_each name="$_FoundObject" in="$_FoundObjects">
            <create_position name="$_Position" object="$_FoundObject" space="$_FoundObject.sector"/>
            <append_to_list name="$_Objects" exact="$_Position"/>
         </do_for_each>

         <do_if value="$DEBUG">
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'"/>
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'$_Objects: ' + $_Objects"/>
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
         </do_if>

         <create_list name="$_Satellites"/>

         <do_while value="$_Objects.count gt 0">
            <set_value name="$_MaxDistanceObject" exact="$_Objects.{1}"/>
            <set_value name="$_MaxDistance" exact="sqrt(($_MaxDistanceObject.x)^2 + ($_MaxDistanceObject.y)^2 + ($_MaxDistanceObject.z)^2)"/>
            <do_for_each name="$_Object" in="$_Objects">
               <set_value name="$_Distance" exact="sqrt(($_Object.x)^2 + ($_Object.y)^2 + ($_Object.z)^2)"/>
               <do_if value="$_Distance gt $_MaxDistance">
                  <set_value name="$_MaxDistance" exact="$_Distance"/>
                  <set_value name="$_MaxDistanceObject" exact="$_Object"/>
               </do_if>
            </do_for_each>
            <append_to_list name="$_Satellites" exact="$_MaxDistanceObject"/>
            <create_list name="$_RemainingObjects"/>
            <do_for_each name="$_Object" in="$_Objects">
               <set_value name="$_Distance" exact="sqrt(($_Object.x - $_MaxDistanceObject.x)^2 + ($_Object.y - $_MaxDistanceObject.y)^2 + ($_Object.z - $_MaxDistanceObject.z)^2)"/>
               <do_if value="$_Distance gt $_RadarRange">
                  <append_to_list name="$_RemainingObjects" exact="$_Object"/>
               </do_if>
            </do_for_each>
            <set_value name="$_Objects" exact="$_RemainingObjects"/>
         </do_while>-->

         <set_value name="$_RadarRange" exact="if $RESTOCK_SATELLITES_MK1 then 30km else 75km"/>
         <create_list name="$_Objects"/>
         <do_for_each name="$_FoundObject" in="$_FoundObjects">
            <create_position name="$_Position" object="$_FoundObject" space="$_FoundObject.sector"/>
            <append_to_list name="$_Objects" exact="$_Position"/>
         </do_for_each>

         <do_if value="$DEBUG">
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'"/>
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'$_Objects: ' + $_Objects"/>
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
         </do_if>

         <create_list name="$_Satellites"/>

         <do_while value="$_Objects.count gt 0">
            <set_value name="$_MaxCoverObject" exact="$_Objects.{1}"/>
            <set_value name="$_MaxCover" exact="0"/>
            <do_for_each name="$_Object" in="$_Objects">
               <create_list name="$_ObjectsInRange"/>
               <do_for_each name="$_PotentialObject" in="$_Objects">
                  <set_value name="$_Distance" exact="sqrt(($_PotentialObject.x - $_Object.x)^2 + ($_PotentialObject.y - $_Object.y)^2 + ($_PotentialObject.z - $_Object.z)^2)"/>
                  <do_if value="$_Distance le $_RadarRange">
                     <append_to_list name="$_ObjectsInRange" exact="$_PotentialObject"/>
                  </do_if>
               </do_for_each>
               <do_if value="$_ObjectsInRange.count gt $_MaxCover">
                  <set_value name="$_MaxCover" exact="$_ObjectsInRange.count"/>
                  <set_value name="$_MaxCoverObject" exact="$_Object"/>
               </do_if>
            </do_for_each>
            <append_to_list name="$_Satellites" exact="$_MaxCoverObject"/>
            <create_list name="$_RemainingObjects"/>
            <do_for_each name="$_Object" in="$_Objects">
               <set_value name="$_Distance" exact="sqrt(($_Object.x - $_MaxCoverObject.x)^2 + ($_Object.y - $_MaxCoverObject.y)^2 + ($_Object.z - $_MaxCoverObject.z)^2)"/>
               <do_if value="$_Distance gt $_RadarRange">
                  <append_to_list name="$_RemainingObjects" exact="$_Object"/>
               </do_if>
            </do_for_each>
            <set_value name="$_Objects" exact="$_RemainingObjects"/>
         </do_while>

         <do_if value="$DEBUG">
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'"/>
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'$_Satellites: ' + $_Satellites"/>
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
         </do_if>

         <do_for_each name="$_Satellite" in="$_Satellites">
            <wait exact="2s"/>
            <set_value name="$LOCATION" exact="[$SECTOR, $_Satellite]"/>
         </do_for_each>

         <do_if value="$DEBUG">
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~'"/>
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'$_Satellites: ' + $_Satellites"/>
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
         </do_if>

         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

         <set_value name="$Quit" exact="true"/>

         <do_if value="$Quit">
            <resume label="FINISH_LBL"/>
         </do_if>
         <resume label="RESUME_LBL"/>

         <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

         <label name="FINISH_LBL"/>
         <wait min="500ms" max="1000ms"/>
         <do_if value="$DEBUG gt 0">
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'[' + player.systemtime.{'%F - %X'} + '] ' + '~~ ' + $DebugScriptName + '.xml ~ Finished ~~'"/>
            <debug_to_file directory="$DebugFolderName" name="$DebugFileName" text="'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~\n'"/>
         </do_if>

         <label name="CLEANUP_LBL"/>
         <wait min="500ms" max="1000ms"/>
         <remove_value name="$DebugFileName"/>
         <remove_value name="$DebugScriptName"/>
         <remove_value name="$DebugFolderName"/>
         <remove_value name="$Quit"/>
         <remove_value name="$Ship"/>
         <remove_value name="$_FoundObject"/>

         <label name="END_LBL"/>
         <wait min="500ms" max="1000ms"/>
         <set_order_syncpoint_reached order="this.ship.defaultorder"/>
         <cancel_order order="this.ship.defaultorder"/>

      </actions>
   </attention>

   <on_abort>
      <remove_value name="$_FoundStationsSector"/>
      <remove_value name="$_FoundStation"/>
      <remove_value name="$DebugFileName"/>
      <remove_value name="$DebugScriptName"/>
      <remove_value name="$DebugFolderName"/>
      <remove_value name="$Quit"/>
      <remove_value name="$Ship"/>
   </on_abort>

</aiscript>